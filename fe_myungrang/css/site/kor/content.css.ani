/* 홈페이지가이드_로그인 */
.login_form {background:url(../../../images/site/kor/content/login_bg.jpg) no-repeat center top; background-size:cover; padding:70px 0 130px 0;}
.login_form .cont {background:#fff; max-width:720px; margin:0 auto; box-sizing:border-box; padding:65px 110px;}
