/* 매장안내_우수매장 */
.store .summery {position:relative;}
.store .summery p.h3 {margin-bottom:25px;}
.store .summery p.h3 strong {margin-bottom:0;}
.store .summery p {margin-right:0; position:relative; z-index:10;}
.store .summery ul {margin-top:40px; position:relative; z-index:10;}
.store .summery ul:after {content:""; display:block; clear:both;}
.store .summery ul li {float:left; width:31%; margin-left:3.5%;}
.store .summery ul li:first-child {margin-left:0;}
.store .summery ul li div {background:#fff; padding:20px 15px; box-shadow:0 5px 20px rgba(0,0,0,0.1); -webkit-box-shadow:0 5px 20px rgba(0,0,0,0.1);}
.store .summery ul li div p {margin:0;}
