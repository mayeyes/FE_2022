/* 창업안내_창업절차 */
.founded_step {}
.founded_step .summery .layout {padding:95px 580px 95px 0; background:url(../../../images/site/kor/content/founded_step_summery_bg.png) no-repeat right 95px;}
.founded_step .summery p.h3 {margin-bottom:35px;}
.founded_step .summery .btns {margin-top:50px;}
.founded_step .step {background:#efefef url(../../../images/site/kor/content/founded_step_bg.jpg) no-repeat right top; background-size:auto 100%; padding:70px 0;} 
.founded_step .step ul {position:relative;}
.founded_step .step ul:before {content:""; display:block; position:absolute; top:58px; width:100%; box-sizing:border-box; border:8px solid #fff; border-left:0; height:318px; border-radius:0 160px 160px 0; -webkit-border-radius:0 160px 160px 0;}
.founded_step .step ul:after {content:""; display:block; clear:both;}
.founded_step .step ul li {float:left; width:25%; min-height:225px; position:relative;}
.founded_step .step ul li:after {content:""; display:block; width:34px; height:34px; background:url(../../../images/site/kor/content/founded_step_arrow.png) no-repeat center 50%; border-radius:50%; -webkit-border-radius:50%; position:absolute; right:50%; top:45px; margin-right:-79px;}
.founded_step .step ul li.step4:after {top:200px; transform:rotate(90deg); -webkit-transform:rotate(90deg); right:0; margin-right:-14px;}
.founded_step .step ul li p {width:124px; text-align:center; position:relative;}
.founded_step .step ul li p:before {content:""; display:block; width:124px; height:124px; margin-bottom:20px; background-color:#fff; background-repeat:no-repeat; background-position:center 50%; background-size:100% auto; border-radius:50%; -webkit-border-radius:50%;}
.founded_step .step ul li.step1 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico1.png);}
.founded_step .step ul li.step2 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico2.png);}
.founded_step .step ul li.step3 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico3.png);}
.founded_step .step ul li.step4 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico4.png);}
.founded_step .step ul li.step5 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico5.png);}
.founded_step .step ul li.step6 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico6.png);}
.founded_step .step ul li.step7 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico7.png);}
.founded_step .step ul li.step8 p:before {background-image:url(../../../images/site/kor/content/founded_step_ico8.png);}
.founded_step .step ul li p span {display:block; color:#c00516; margin-bottom:15px;}
.founded_step .step ul li p strong {display:block; font-weight:400; color:#222;}
.founded_step .step ul li + li + li + li + li {float:right; margin-top:85px;}
.founded_step .step ul li.step5:after {display:none;} 
.founded_step .step ul li.step6:after,
.founded_step .step ul li.step7:after,
.founded_step .step ul li.step8:after {transform:rotate(180deg); -webkit-transform:rotate(180deg);} 

