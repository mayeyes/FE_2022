.store.good .summery p.h3 strong {display:inline-block; vertical-align:top}
.store.good .summery p.h3 span {display:inline-block; vertical-align:top; margin:8px 0 0 10px; background:#665d58; color:#fff; font-size:19px; line-height:23px; padding:9px 20px; border-radius:21px; -webkit-border-radius:21px;}
.store.new .summery p {margin-right:0;}
