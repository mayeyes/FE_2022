@charset "utf-8";

/*** Animation ***/
.cont .summery p.h3 strong,
.cont .summery p.h3 span,

.cooperative .summery:after,
.cooperative .summery .vision li:before,
.cooperative .ci .symbol img,

.our_story .pr li,
.cooperative .ci .brands ul li,
.cooperative .ci .brands ul li a:before
{
	transition : all 0.3s ease;
	-webkit-transition : all 0.3s ease;
}

/* 공통 */
.cont .summery {} 
.cont .summery .layout {padding:95px 0;}
.cont .summery p.h3 {color:#222;}
.cont .summery p.h3 strong {display:block; font-size:45px; line-height:52px; font-weight:500; margin-bottom:8px;}
.cont .summery p.h3 span {display:block; font-size:35px; line-height:42px; font-weight:300;} 

/* 명랑시대_명랑시대협동조합 */
.cooperative .summery {background:#f3f1ef url(../../../images/site/kor/content/cooperative_bg.jpg) no-repeat right top; background-size:auto 100%; position:relative;}
.cooperative .summery:after {content:""; display:block; width:639px; height:625px; background:url(../../../images/site/kor/content/cooperative_bg2.png) no-repeat center 50%; background-size:100% auto; position:absolute; right:50%; margin-right:-900px; top:0;}
.cooperative .summery p {margin-right:310px; position:relative; z-index:10;}
.cooperative .summery p.h3 {margin-bottom:25px;}
.cooperative .summery .vision {padding-top:10px; position:relative; z-index:10;}
.cooperative .summery .vision:after {content:""; display:block; clear:both;}
.cooperative .summery .vision li {float:left; text-align:center; margin-left:20px; color:#c00516; font-size:20px; font-weight:500;}
.cooperative .summery .vision li:first-child {margin-left:0;}
.cooperative .summery .vision li:before {content:""; display:block; width:130px; height:130px; background-color:#fff; background-repeat:no-repeat; background-position: center 50%; background-size:100% auto; margin-bottom:20px; border-radius:50%; -webkit-border-radius:50%;}
.cooperative .summery .vision li span {display:block; color:#222; opacity:0.7; -webkit-opacity:0.7; font-weight:300; font-size:17px; margin-top:5px;}
.cooperative .summery .vision li.bn1:before {background-image:url(../../../images/site/kor/content/vision_ico1.png);}
.cooperative .summery .vision li.bn2:before {background-image:url(../../../images/site/kor/content/vision_ico2.png);}
.cooperative .summery .vision li.bn3:before {background-image:url(../../../images/site/kor/content/vision_ico3.png);}
.cooperative .summery .vision li.bn4:before {background-image:url(../../../images/site/kor/content/vision_ico4.png);}
.cooperative .summery .vision li.bn5:before {background-image:url(../../../images/site/kor/content/vision_ico5.png);}
.cooperative .ci {padding:95px 0;}
.cooperative .ci .symbol {border:2px solid #ccc; margin-top:35px; padding:45px; background:url(../../../images/site/kor/content/symbol_gridbg.gif) repeat -1px -1px;}
.cooperative .ci .symbol img {display:block; margin:0 auto; max-width:100%;}
.cooperative .ci .color {background:#f3f3f3; padding:50px 5%; text-align:center;}
.cooperative .ci .color li {display:inline-block; vertical-align:top;}
.cooperative .ci .color li p {padding:15px 0 0 95px; font-size:18px; height:77px; box-sizing:border-box; position:relative; text-align:left; color:#333;}
.cooperative .ci .color li p:before {content:""; display:block; width:77px; height:77px; position:absolute; left:0; top:0; border-radius:50%; -webkit-border-radius:50%; background-image:url(../../../images/site/kor/content/ci_color_ico.png); background-color:#e7102d; background-size:100% auto; background-repeat:no-repeat; background-position: center 50%;}
.cooperative .ci .color li p span {font-size:16px; display:block;}
.cooperative .ci .color li:first-child {margin-right:40px;}
.cooperative .ci .color li:first-child p:before {background-color:#000;}

/*브랜드 소개*/
.cooperative .ci .brands{background-color:#f3f3f3;}
.cooperative .ci .brands ul{overflow:hidden; max-width:980px; width:100%; margin:0 auto; padding:40px 35px; box-sizing:border-box;}
.cooperative .ci .brands ul li{position:relative; float:left; width:calc((100% - 10%) / 2); min-height:120px; padding-left:calc(170px + 4%); box-sizing:border-box;}
.cooperative .ci .brands ul li:first-child{margin-right:10%;}
.cooperative .ci .brands ul li a{text-decoration:none;}
.cooperative .ci .brands ul li a:before{content:""; position:absolute; top:0; left:0; width:170px; height:120px; background-color:#fff; background-repeat:no-repeat; background-position:center; border-radius:10px;}
.cooperative .ci .brands ul li:first-child a:before{background-image:url(../../../images/site/kor/content/brands_img01.svg);}
.cooperative .ci .brands ul li:last-child a:before{background-image:url(../../../images/site/kor/content/brands_img02.svg);}
.cooperative .ci .brands ul li span{display:block; font-size:16px; font-weight:500; color:#222;}
.cooperative .ci .brands ul li em{display:inline-block; height:28px; line-height:28px; margin-top:22px; padding:0 18%; background-color:#222; border-radius:14px; font-style:normal; font-size:12px; text-align:center; color:#fff; box-sizing:border-box;}

/* 명랑시대_명랑시대쌀핫도그 */
.cooperative.bi .summery {background:url(../../../images/site/kor/content/cooperative_bg2.jpg) no-repeat center top; background-size:cover;}
.cooperative.bi .summery .layout {padding-bottom:370px;} 
.cooperative.bi .summery:after {display:none;}
.cooperative.bi .summery p {margin-right:0;}
.cooperative.bi .summery .concept {width:100%; position:absolute; left:0; bottom:-175px;}
.cooperative.bi .summery .concept:after {content:""; display:block; clear:both;}
.cooperative.bi .summery .concept li {float:left; width:33.333%; position:relative; text-align:center;}
.cooperative.bi .summery .concept li:nth-child(2) {margin-top:50px;}
.cooperative.bi .summery .concept li span {position:absolute; left:30px; top:30px; width:70px; height:70px; line-height:70px; color:#fff; text-align:center; font-size:20px; background:rgba(96,65,11,0.8); border-radius:50%; -webkit-border-radius:50%;}
.cooperative.bi .summery .concept li .photo {box-shadow:7px 7px 20px rgba(0,0,0,0.2); -webkit-box-shadow:7px 7px 20px rgba(0,0,0,0.2); overflow:hidden;}
.cooperative.bi .summery .concept li img {display:block; height:390px;}
.cooperative.bi .summery .concept li p {margin-top:35px; font-size:20px; color:#222; font-weight:300;}
.cooperative.bi .summery .concept li p strong {font-weight:500;}
.cooperative.bi .ci {padding-top:250px;}
.cooperative.bi .ci .symbol {padding:80px 45px;}
.cooperative.bi .ci .color.bi li p:before {background-color:#221E1F; background-image:url(../../../images/site/kor/content/bi_color_ico.png);}
.cooperative.bi .ci .color li:first-child p:before {background-color:#ED3129;}
.cooperative .ci .appli {}
.cooperative .ci .appli:after {content:""; display:block; clear:both;}
.cooperative .ci .appli li {float:left; margin-right:2%; width:32%; box-sizing:border-box; font-size:20px; margin-bottom:30px;}
.cooperative .ci .appli li:nth-child(3n) {margin-right:0;}
.cooperative .ci .appli li span {display:block; border:1px solid #cecece; margin-bottom:10px;}
.cooperative .ci .appli li span img {display:block; width:100%;}

/* 명랑시대_사회공헌 */
.contribution .summery {background:#f5f5f5 url(../../../images/site/kor/content/contribution_bg.jpg) no-repeat right top; background-size:auto 100%; position:relative;}
.contribution .summery p.h3 {margin-bottom:30px;}
.contribution .summery p {margin-right:35%;}
.contribution .summery p.email {font-size:18px; color:#222222; margin-top:30px; background:url(../../../images/site/kor/content/email_ico.png) no-repeat left top; padding:0 0 0 30px;}
.contribution .summery p.email a {color:#222222;}

/* 메뉴_소스 */
.sauce .layout {padding:95px 0;}
.sauce h4 {margin-top:0;}
.sauce .kinds {background:url(../../../images/site/kor/content/sauce_bg1.jpg) no-repeat center 50%; background-size:cover;}
.sauce .kinds ul {}
.sauce .kinds ul:after {content:""; display:block; clear:both;}
.sauce .kinds ul li {float:left; width:20%; text-align:center; font-size:18px; color:#222; font-weight:500; margin-top:40px;}
.sauce .kinds ul li img {display:block; margin:0 auto; transform:translateX(7px); -webkit-transform:translateX(7px);}
.sauce .kinds ul li span {display:block; font-weight:400; opacity:0.7; -webkit-opacity:0.7; margin-top:5px;}
.sauce .recommend {background:url(../../../images/site/kor/content/sauce_bg2.jpg) no-repeat center 50%; background-size:cover; position:relative;}
.sauce .recommend:after {content:""; display:block; width:100%; height:100%; position:absolute; left:0; top:0; background:url(../../../images/site/kor/content/sauce_patternbg.png) repeat;}
.sauce .recommend h4,
.sauce .recommend p {color:#fff; position:relative; z-index:10;}
.sauce .recommend ul {padding:70px 0 0 0; position:relative; z-index:10;}
.sauce .recommend ul:after {content:""; display:block; clear:both;}	
.sauce .recommend ul li {float:left; width:32%; margin-left:2%; position:relative; box-sizing:border-box; border:1px solid #fff; background:rgba(0,0,0,0.2); height:230px; display:table;}
.sauce .recommend ul li:first-child {margin-left:0;}
.sauce .recommend ul li span.rank {position:absolute; left:7%; top:-37px; z-index:10; display:block; width:77px; height:77px; box-sizing:border-box; padding:8px 0 0 0; font-size:15px; border-radius:50%; border-radius:50%; color:#e7a5a8; text-align:center; background:#ce081a;}
.sauce .recommend ul li span.rank strong {display:block; font-size:35px; line-height:35px; color:#fff;}
.sauce .recommend ul li p {display:table-cell; vertical-align:middle; width:100%; text-align:center; box-sizing:border-box; padding:0 10%;}
.sauce .recommend ul li p span {display:block; padding:105px 0 0 0;}
.sauce .recommend ul li.bn1 p span {background:url(../../../images/site/kor/content/recommend_ico1.png) no-repeat center top;}
.sauce .recommend ul li.bn2 p span {background:url(../../../images/site/kor/content/recommend_ico2.png) no-repeat center top;}
.sauce .recommend ul li.bn3 p span {background:url(../../../images/site/kor/content/recommend_ico3.png) no-repeat center top;}

/* 메뉴_기타 */
.etc_menu {padding:80px 0;}
.etc_menu ul {}
