.store.new .summery p.link {font-size:19px; line-height:24px; color:#2c3137; position:absolute; right:0; top:104px;}	
.store.new .summery p.link .btn {min-width:10px; padding:11px 25px; margin-left:10px;}
.store.new .summery p.link .btn:link:hover,
.store.new .summery p.link .btn:visited:hover,
.store.new .summery p.link .btn:active:hover{background:none;}
.store.new .summery p.title {text-align:left;}
.store.new .summery p.title:after {content:""; display:block; clear:both;}
.store.new .summery p.title strong em {font-style:normal; margin-right:7px; background:#d61f26; color:#fff; text-align:center; display:inline-block; padding:4px 12px; border-radius:5px; -webkit-border-radius:5px; font-size:16px; font-weight:400;}
.store.new .summery p.title span {color:#d61f26; font-size:17px; text-align:right; float:right; margin-top:2px;}
.store.new .layout {padding:95px 0;}
.store.new .layout h4 {margin-top:0;}
.store.new .entry_list {}
.store.new .entry_list li {position:relative; padding:22px 0 22px 100px; border-bottom:1px dashed #dddddd; font-size:18px; min-height:38px; box-sizing:border-box;}
.store.new .entry_list li strong {display:block; position:absolute; left:0; top:50%; margin-top:-19px; border-radius:5px; -webkit-border-radius:5px; text-align:center; width:85px; background:#6b625d; color:#fff; font-size:20px; height:38px; line-height:38px; font-weight:500;}

